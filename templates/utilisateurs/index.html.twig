{% extends 'base.html.twig' %}

{% block title %}Utilisateurs{% endblock %}

{% block body %}

<div>
    <h1>  Liste des utilisateurs</h1>

    <ul>
       {% for user in users %}
       <li>{{ user.prenom }}&nbsp{{ user.nom }}</li>
       <li>{{ user.email }} </li>
       <li>{{ user.profession }} </li>
       <li>{{ user.dateNaissance | date ("d/m/Y") }} </li><br> 
       <br>
       {% endfor %}
    </ul>
</div>
{% endblock %}